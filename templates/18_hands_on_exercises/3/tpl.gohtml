<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
</head>
<body>

{{range .}}
    <h1>{{.Name}}</h1>
    <section>
        <h2>Breakfast</h2>
        <ul>
            {{range .Breakfast}}
                <li>{{.}}</li>
            {{end}}
        </ul>
        <h2>Lunch</h2>
        <ul>
            {{range .Lunch}}
                <li>{{.}}</li>
            {{end}}
        </ul>
        <h2>Dinner</h2>
        <ul>
            {{range .Dinner}}
                <li>{{.}}</li>
            {{end}}
        </ul>
    </section>
{{end}}

</body>
</html>